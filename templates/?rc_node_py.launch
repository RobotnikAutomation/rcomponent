<?xml version="1.0"?>
<launch>

	<arg name="id_robot" default="robot"/>
	<!-- Desired frequency -->
	<arg name="desired_freq" default="1.0" />
	<!-- Name of the example topic -->
	<arg name="example_topic_name" default="example" />

	<group ns="$(arg id_robot)">

		<!-- start ?RCNode node -->
		<node name="?rc_node" pkg="?rc_package" type="?rc_node_node.py" output="screen" respawn="false">
			<param name="desired_freq" value="$(arg desired_freq)"/>
			<param name="example_topic_name" value="$(arg example_topic_name)"/>
		</node>

	</group>

</launch>
